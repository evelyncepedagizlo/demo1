<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--    Secuencia para Asamblea  -->
        <record id="sequence_asamblea" model="ir.sequence">
            <field name="name">Secuencia para Asamblea</field>
            <field name="code">asamblea</field>
            <field name="prefix">Asamblea - </field>
            <field name="padding">8</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <!--    Secuencia para Entrega Vehiculo  -->
        <record id="sequence_entrega_vehiculo" model="ir.sequence">
